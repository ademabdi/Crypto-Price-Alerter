
import requests
import json

response = requests.get("https://api.coingecko.com/api/v3/coins/markets?vs_currency=usd&order=market_cap_desc&per_page=100&page=1&sparkline=false&price_change_percentage=24h")
data = json.loads(response.content)

for i in data:
    for k,v in i.items():
        print(f" ID: {i['id']}, SYMBOL: {i['symbol']}, PRICE: ${i['current_price']}, MARKET CAP: {i['market_cap']}, MARKET CAP RANK: {i['market_cap_rank']}")
        break